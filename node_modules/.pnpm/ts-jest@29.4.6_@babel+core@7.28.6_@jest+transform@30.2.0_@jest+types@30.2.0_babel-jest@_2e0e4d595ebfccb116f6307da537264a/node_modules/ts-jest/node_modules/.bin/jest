#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/402292/.gemini/antigravity/playground/emerald-aurora/booking-system/node_modules/.pnpm/jest@30.2.0_@types+node@22.19.7_ts-node@10.9.2_@types+node@22.19.7_typescript@5.9.3_/node_modules/jest/bin/node_modules:/Users/402292/.gemini/antigravity/playground/emerald-aurora/booking-system/node_modules/.pnpm/jest@30.2.0_@types+node@22.19.7_ts-node@10.9.2_@types+node@22.19.7_typescript@5.9.3_/node_modules/jest/node_modules:/Users/402292/.gemini/antigravity/playground/emerald-aurora/booking-system/node_modules/.pnpm/jest@30.2.0_@types+node@22.19.7_ts-node@10.9.2_@types+node@22.19.7_typescript@5.9.3_/node_modules:/Users/402292/.gemini/antigravity/playground/emerald-aurora/booking-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/402292/.gemini/antigravity/playground/emerald-aurora/booking-system/node_modules/.pnpm/jest@30.2.0_@types+node@22.19.7_ts-node@10.9.2_@types+node@22.19.7_typescript@5.9.3_/node_modules/jest/bin/node_modules:/Users/402292/.gemini/antigravity/playground/emerald-aurora/booking-system/node_modules/.pnpm/jest@30.2.0_@types+node@22.19.7_ts-node@10.9.2_@types+node@22.19.7_typescript@5.9.3_/node_modules/jest/node_modules:/Users/402292/.gemini/antigravity/playground/emerald-aurora/booking-system/node_modules/.pnpm/jest@30.2.0_@types+node@22.19.7_ts-node@10.9.2_@types+node@22.19.7_typescript@5.9.3_/node_modules:/Users/402292/.gemini/antigravity/playground/emerald-aurora/booking-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest@30.2.0_@types+node@22.19.7_ts-node@10.9.2_@types+node@22.19.7_typescript@5.9.3_/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../jest@30.2.0_@types+node@22.19.7_ts-node@10.9.2_@types+node@22.19.7_typescript@5.9.3_/node_modules/jest/bin/jest.js" "$@"
fi
